{
  "Connstring": "Data Source=localhost;Port=3306;User ID=root;Password=123456;Initial Catalog=gr_test;Charset=utf8;SslMode=none;Max pool size=5",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "rediscache": {
    "redisconnectionstring": "localhost:6379,connectTimeout=5000,connectRetry=3,syncTimeout=5000,defaultDatabase=1", //redis数据库连接字符串
